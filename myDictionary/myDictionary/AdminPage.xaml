<Window x:Class="myDictionary.AdminPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:myDictionary"
        mc:Ignorable="d"
        Title="AdminPage" Height="450" Width="800">
    <Grid>
        <Label Content="Password" HorizontalAlignment="Left" Margin="118,0,0,0" VerticalAlignment="Center" FontSize="20" RenderTransformOrigin="-0.097,0.592"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="118,137,0,0" VerticalAlignment="Top" FontSize="20"/>
        <TextBox x:Name="usernameBox" HorizontalAlignment="Left" Margin="243,141,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="434" Height="36"/>
        <TextBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="243,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="434" Height="36"/>
        <Button x:Name="verifyButton" Content="Verify" HorizontalAlignment="Left" Margin="283,310,0,0" VerticalAlignment="Top" Width="239" Height="81" FontSize="22" Click="verifyButton_Click" />

    </Grid>

        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:myDictionary"
        mc:Ignorable="d"
        Title="AdminInterface" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="15*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Button x:Name="addButton" Content="Add" HorizontalAlignment="Left" Margin="37,235,0,0" VerticalAlignment="Top" Width="199" Height="69" Click="addButton_Click"/>
        <Button x:Name="modifyButton" Content="Modify" HorizontalAlignment="Left" Margin="262,235,0,0" VerticalAlignment="Top" Width="181" Height="69" Click="modifyButton_Click"/>
        <Button x:Name="deleteButton" Content="delete" HorizontalAlignment="Left" Margin="484,235,0,0" VerticalAlignment="Top" Width="230" Height="69" Click="deleteButton_Click"/>

    </Grid>
</Window>
